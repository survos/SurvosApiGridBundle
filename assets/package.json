{
  "name": "@survos/api-grid-bundle",
  "description": "integrate php and javascript helpers for DataTables.net",
  "license": "MIT",
  "version": "1.0.1",
  "symfony": {
    "controllers": {
      "api_grid": {
        "main": "src/controllers/api_grid_controller.js",
        "webpackMode": "eager",
        "fetch": "lazy",
        "enabled": true,
        "autoimport": {
          "datatables.net-bs5/css/dataTables.bootstrap5.min.css": true,
          "datatables.net-responsive/js/dataTables.responsive.min.js": true,
          "datatables.net-responsive-bs5/js/responsive.bootstrap5.min.js": true,
          "datatables.net-responsive-bs5/css/responsive.bootstrap5.min.css": true,
          "datatables.net-fixedheader-bs5/css/fixedHeader.bootstrap5.min.css": true,
          "datatables.net-fixedheader-bs5/js/fixedHeader.bootstrap5.min.js": true,
          "datatables.net-searchbuilder-bs5/css/searchBuilder.bootstrap5.min.css": true
        }
      }
    }
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.1.0"
  },
  "devDependencies": {
    "@hotwired/stimulus": "^3.1.0"
  },
