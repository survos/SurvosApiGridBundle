    "@popperjs/core": "^2.11.5",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "datatables.net": "^1.12.1",
    "datatables.net-bs5": "^1.10.25",
    "datatables.net-buttons-bs5": "^2.2",
    "datatables.net-colreorder": "^1.5.4",
