{
    "name": "survos/api-grid-bundle",
    "description": "incorporate DatatablesNet, using twig and stimulus, twig_component requires ^2.2",
    "keywords": [
        "symfony-ux",
        "symfony6"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.12",
        "doctrine/persistence": "^2.0 || ^3.0",
        "survos/grid-bundle": "^1.3",
        "survos/inspection-bundle": "^1.3",
        "friendsofsymfony/jsrouting-bundle": "^3.2",
        "symfony/config": "^6.1.0",
        "symfony/dependency-injection": "^6.1.0",
        "symfony/http-kernel": "^6.1.0",
        "symfony/options-resolver": "^6.1.0",
        "symfony/ux-twig-component": "^2.3",
        "twig/twig": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Survos\\ApiGrid\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.3-dev"
        },
        "symfony": {
            "require": "^6.1.0"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow",
        "api-platform/core": "For javascript-based grid rendering",
        "friendsofsymfony/jsrouting-bundle": "use path() in javascript grid rendering"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpstan/phpstan": "^1.8",
        "symfony/browser-kit": "^6.1.0",
        "symfony/framework-bundle": "^6.1.0",
        "symfony/phpunit-bridge": "^6.1.0",
        "symfony/twig-bundle": "^6.1.0",
        "symfony/var-dumper": "^6.1.0",
        "symfony/webpack-encore-bundle": "^1.11"
    },
    "conflict": {
        "survos/maker-bundle": "<1.2.44",
        "survos/html-prettify-bundle": "<1.2.44",
        "survos/barcode-bundle": "<1.2.44",
        "survos/bootstrap-bundle": "<1.2.44",
        "survos/faker-bundle": "<1.2.44",
        "survos/doc-bundle": "<1.2.44",
        "survos/core-bundle": "<1.2.44",
        "survos/tree-bundle": "<1.2.44",
        "survos/auth-bundle": "<1.2.44",
        "survos/workflow-helper-bundle": "<1.2.44",
        "survos/crawler-bundle": "<1.2.44",
        "survos/location-bundle": "<1.2.44",
        "survos/command-bundle": "<1.2.44",
        "survos/ruler-bundle": "<1.2.44",
        "survos/providence-bundle": "<1.2.44",
        "survos/stripe-product": "<1.2.44",
        "survos/wiki-bundle": "<1.2.44",
        "survos/import-bridge": "<1.2.44",
        "survos/grid-group-bundle": "<1.2.44"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
